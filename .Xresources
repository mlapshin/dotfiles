URxvt*font: xft:DejaVu Sans Mono:pixelsize=14
URxvt*letterSpace: -1
URxvt*internalBorder: 1

!URxvt*transparent: true
!URxvt*shading: 10
URxvt*scrollBar: false
URxvt*scrollstyle: plain

URxvt.keysym.A-w: builtin:
URxvt.keysym.C-y: builtin:
URxvt.keysym.C-A-y: builtin:
URxvt.keysym.M-u: builtin:
URxvt.keysym.M-Escape: builtin:
URxvt.keysym.M-s: \033<M-s>
URxvt*iso14755: False

Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.rgba: grb
Xft.autohint: 1
Xft.lcdfilter: lcddefault

Emacs.font: DejaVu Sans Mono-10
